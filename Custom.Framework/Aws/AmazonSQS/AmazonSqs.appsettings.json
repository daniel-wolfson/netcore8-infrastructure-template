{
  "AmazonSQS": {
    "// Region": "AWS region where SQS queues are located",
    "Region": "us-east-1",
    
    "// AccessKey": "AWS Access Key (optional if using IAM roles/instance profiles)",
    "AccessKey": null,
    
    "// SecretKey": "AWS Secret Key (optional if using IAM roles/instance profiles)",
    "SecretKey": null,
    
    "// ServiceUrl": "Custom endpoint for local SQS (e.g., LocalStack), leave null for AWS",
    "ServiceUrl": null,
    
    "// DefaultQueueName": "Default queue name for operations",
    "DefaultQueueName": "default-queue",
    
    "// EnableBatchProcessing": "Enable batch operations for better performance",
    "EnableBatchProcessing": true,
    
    "// MaxBatchSize": "Maximum items per batch (SQS limit is 10)",
    "MaxBatchSize": 10,
    
    "// MaxNumberOfMessages": "Maximum messages to receive per request (1-10)",
    "MaxNumberOfMessages": 10,
    
    "// MaxRetries": "Number of automatic retries for failed operations",
    "MaxRetries": 3,
    
    "// TimeoutSeconds": "Request timeout in seconds",
    "TimeoutSeconds": 30,
    
    "// EnableMetrics": "Enable CloudWatch metrics collection",
    "EnableMetrics": true,
    
    "// WaitTimeSeconds": "Long polling wait time (0-20 seconds)",
    "WaitTimeSeconds": 20,
    
    "// VisibilityTimeoutSeconds": "Message visibility timeout (0-43200 seconds)",
    "VisibilityTimeoutSeconds": 30,
    
    "// EnableFifoQueues": "Enable FIFO queue support",
    "EnableFifoQueues": false,
    
    "// EnableContentBasedDeduplication": "Enable content-based deduplication for FIFO queues",
    "EnableContentBasedDeduplication": true,
    
    "// EnableDeadLetterQueue": "Enable dead letter queue for failed messages",
    "EnableDeadLetterQueue": true,
    
    "// MaxReceiveCount": "Maximum receive count before moving to dead letter queue",
    "MaxReceiveCount": 3,
    
    "// DeadLetterQueueSuffix": "Suffix for dead letter queue names",
    "DeadLetterQueueSuffix": "-dlq"
  },

  "// Example configurations for different environments": "",
  
  "AmazonSQS:Production": {
    "Region": "us-east-1",
    "DefaultQueueName": "prod-orders",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeoutSeconds": 60,
    "EnableMetrics": true,
    "EnableDeadLetterQueue": true,
    "MaxReceiveCount": 5
  },

  "AmazonSQS:Staging": {
    "Region": "us-west-2",
    "DefaultQueueName": "staging-orders",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeoutSeconds": 30,
    "EnableMetrics": true,
    "EnableDeadLetterQueue": true,
    "MaxReceiveCount": 3
  },

  "AmazonSQS:Development": {
    "Region": "us-east-1",
    "ServiceUrl": "http://localhost:4566",
    "AccessKey": "test",
    "SecretKey": "test",
    "DefaultQueueName": "dev-orders",
    "MaxNumberOfMessages": 5,
    "WaitTimeSeconds": 5,
    "VisibilityTimeoutSeconds": 30,
    "EnableMetrics": false,
    "MaxRetries": 1
  },

  "// Sample queue configurations": "",
  
  "Queues": {
    "Orders": {
      "QueueName": "orders-queue",
      "Type": "Standard",
      "VisibilityTimeout": 60,
      "MessageRetentionPeriod": 345600,
      "MaximumMessageSize": 262144,
      "DelaySeconds": 0,
      "DeadLetterQueue": "orders-queue-dlq",
      "MaxReceiveCount": 3,
      "Description": "High-volume order processing queue"
    },
    
    "OrdersFifo": {
      "QueueName": "orders-queue.fifo",
      "Type": "FIFO",
      "FifoQueue": true,
      "ContentBasedDeduplication": true,
      "VisibilityTimeout": 60,
      "MessageRetentionPeriod": 345600,
      "DeadLetterQueue": "orders-queue-dlq.fifo",
      "MaxReceiveCount": 3,
      "Description": "FIFO order processing queue with guaranteed ordering"
    },
    
    "Notifications": {
      "QueueName": "notifications-queue",
      "Type": "Standard",
      "VisibilityTimeout": 30,
      "MessageRetentionPeriod": 86400,
      "MaximumMessageSize": 262144,
      "DelaySeconds": 0,
      "DeadLetterQueue": "notifications-queue-dlq",
      "MaxReceiveCount": 5,
      "Description": "Real-time notification delivery queue"
    },
    
    "Jobs": {
      "QueueName": "jobs-queue",
      "Type": "Standard",
      "VisibilityTimeout": 300,
      "MessageRetentionPeriod": 1209600,
      "MaximumMessageSize": 262144,
      "DelaySeconds": 0,
      "DeadLetterQueue": "jobs-queue-dlq",
      "MaxReceiveCount": 3,
      "Description": "Background job processing queue"
    },
    
    "HighPriority": {
      "QueueName": "high-priority-queue",
      "Type": "Standard",
      "VisibilityTimeout": 30,
      "MessageRetentionPeriod": 86400,
      "MaximumMessageSize": 262144,
      "DelaySeconds": 0,
      "DeadLetterQueue": "high-priority-queue-dlq",
      "MaxReceiveCount": 5,
      "Description": "High priority message processing"
    }
  },

