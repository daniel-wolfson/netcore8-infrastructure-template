{
  "Version": "V124PP",
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "RedisConfig": {
    "ConnectionString": "127.0.0.1:6379,defaultDatabase=0,allowAdmin=true,connectRetry=3"
  },
  "Kafka": {
    "Common": {
      "ServiceShortName": "SERV",
      "BootstrapServers": "kafka1:9092,kafka2:9092",
      "SecurityProtocol": "Plaintext", //"SaslSsl",
      "EnableIdempotence": true,
      "ClientId": "{Environment}-{ServiceShortName}-{Name}-{InstanceIndex}",
      "GroupId": "{ServiceShortName}-{Name}-group"
    },
    "Producers": [
      {
        "Name": "Test",
        "Topics": [ "Test" ],
        "BootstrapServers": "prod-kafka-cluster:9092",
        "BatchSize": 32768, // "default": 1000000,
        "ClientId": "prod-producer",
        "CompressionType": "gzip", // default: "none"
        "CompressionLevel": 6, // default: -1; (0-9)
        "Delivery": "AtLeastOnce",
        "EnableDuplicateDetection": true,
        "EnableMetrics": true,
        "EnableHealthCheck": true,
        "LingerMs": 5, // default: 5
        "MessageSendMaxRetries": 3, // default: 2147483647
        "MessageMaxBytes": 1000000, // "default": 1000000,
        "SaslUsername": "prod-user",
        "SaslPassword": "prod-password",
        "SaslMechanism": "ScramSha256",
        "SecurityProtocol": "SaslSsl",
        "TransactionTimeoutMs": null //default: 60000
      },
      {
        "Name": "DeadLetters",
        "Topics": [ "DeadLetters" ],
        "BootstrapServers": "prod-kafka-cluster:9092",
        "ClientId": "test-producer",
        "MaxAttempts": 3,
        "RetryIntervalSeconds": 10
      }
    ],
    "Consumers": [
      {
        "Name": "Main",
        "GroupId": "consumer-group",
        "Topics": [ "t1" ],
        "BootstrapServers": "prod-kafka-cluster:9092",
        "ClientId": "prod-consumer",
        "EnableMetrics": true,
        "EnableHealthCheck": true,
        "AutoOffsetReset": "Earliest",
        "EnableAutoCommit": false,
        "AutoCommitIntervalMs": 5000, //default: 5000
        "FetchMaxBytes": 52428800, // default: 52428800
        "FetchErrorBackoffMs": 500, //default: 500
        "MaxPartitionFetchBytes": null, //default: 1048576
        "SaslUsername": "prod-user",
        "SaslPassword": "prod-password",
        "SaslMechanism": "ScramSha256",
        "SecurityProtocol": "SaslSsl"
      }
    ]
  },
  "DalConfig": {
    "Host": "https://e1-isrotel-dal.azurewebsites.net"
  },
  "UmbracoConfig": {
    "Host": "https://www.isrotel.co.il/umbraco"
  },
  "OptimaSearchConfig": {
    "Host": "https://secureapi.webbnb.com"
  },
  "OptimaSunClubConfig": {
    "Host": "https://secureapi.webbnb.com"
  }
}
